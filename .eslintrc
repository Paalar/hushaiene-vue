{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/essential",
        "eslint:recommended",
        "@vue/typescript"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off"
    }
}
